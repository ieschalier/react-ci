{
  "extends": "airbnb",
  "settings": {
    "import/resolver": "node"
  },
  "rules": {
    "max-len": ["error", 80],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "import/no-unresolved": 0,
    "no-confusing-arrow": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "semi": [
      1,
      "never"
    ],
  }
}
